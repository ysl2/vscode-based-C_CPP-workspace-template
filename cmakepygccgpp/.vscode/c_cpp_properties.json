{
    "configurations": [
        {
            "name"       : "Win32",
            "includePath": [
                "${workspaceFolder}/src/**"
            ],
            "browse": {
                "path"                         : [],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename"             : ""
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "__GNUC__=6",
                "__cdecl=__attribute__((__cdecl__))"
            ],
            "compilerPath"         : "C:/Program Files/LLVM/bin/g++.exe",
            "cStandard"            : "c11",
            "cppStandard"          : "c++14",
            "intelliSenseMode"     : "gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
