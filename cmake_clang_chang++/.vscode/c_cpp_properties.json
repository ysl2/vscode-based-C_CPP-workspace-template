{
    "configurations": [
        {
            "name"       : "Win32",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename"             : ""
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion"    : "10.0.17763.0",
            "compilerPath"         : "C:/Program Files/LLVM/bin/g++.exe",
            "cStandard"            : "c11",
            "cppStandard"          : "c++14",
            "intelliSenseMode"     : "clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}
